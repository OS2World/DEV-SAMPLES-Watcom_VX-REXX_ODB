somlevel = SOM1

objs = &
    INITTERM.obj &
    SOMINIT.obj &
    BOUNDPB.obj &
    PROPS.obj &

!include F:\VXODK\\bin\makewatc.inc

exports = SOMInitModule SOMUninitModule _VXREXX_CALLER_ _BOUNDPB_VERSION_INFO_
libs = som.lib rexx.lib os2386.lib

$(obj_dir)\$(dllname).dll : $(objs) $(obj_dir)\$(dllname).lnk $(obj_dir)\$(dllname).res
    @cd $(obj_dir)
    $(std_link) @$(dllname)
    rc -p -x $(dllname).res $(dllname).dll > nul
!ifdef DEBUG
    @copy /b $(dllname).dll + $(dllname).sym $(dllname).dll 2>&1 >nul
!endif
    @cd $(main_dir)

$(obj_dir)\$(dllname).res : $(rc_dir)\$(dllname).rc  rc\BOUNDPB.ICO
    @cd $(rc_dir)
    rc -r $[*.rc $(obj_dir)\$[&.res
    @cd $(main_dir)

$(obj_dir)\$(dllname).lnk : makefile $(locallibs)
    %create $@
    @%append $@ option osname='OS/2 32-bit'
    @%append $@ libpath %WATCOM%\lib386
    @%append $@ libpath %WATCOM%\lib386\os2
    @%append $@ libpath %TOOLKIT%\os2lib
    @%append $@ format os2 lx dll initinstance terminstance
    @%append $@ option Quiet,Map,CaseExact,ManyAutoData
    @%append $@ option symfile=$(dllname).sym
    @%append $@ name $(dllname)
    @%append $@ debug all
    @for %i in ($(objs)) do @%append $^@ file %i
    @for %i in ($(locallibs)) do @%append $^@ library %i
    @for %i in ($(libs)) do @%append $^@ library %i
    @for %i in ($(exports)) do @%append $^@ export %i

